{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "operator-linebreak": "off",
        "no-restricted-globals": [
            "error",
            "event",
            "fdescribe"
        ],
        "no-alert": 0,
        "no-param-reassign": 0,
        "linebreak-style": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0,
        "import/prefer-default-export": 0
    }
}